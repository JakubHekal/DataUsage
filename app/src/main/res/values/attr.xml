<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AppTheme">
        <attr name="colorPrimary" format="color"/>
        <attr name="colorSecondary" format="color"/>
        <attr name="colorPrimaryText" format="color"/>
        <attr name="colorSecondaryText" format="color"/>
        <attr name="colorIcon" format="color"/>
        <attr name="colorBorder" format="color"></attr>
        <attr name="colorBackground" format="color"/>
        <attr name="colorCardBackground" format="color"/>
        <attr name="colorLow" format="color"/>
        <attr name="colorMedium" format="color"/>
        <attr name="colorHigh" format="color"/>
    </declare-styleable>

    <declare-styleable name="LineView">
        <attr name="textTitle" format="string"/>
        <attr name="textInfo" format="string"/>
        <attr name="imageIcon" format="reference"/>
        <attr name="tintIcon" format="color"/>
        <attr name="textLabel" format="string"/>
        <attr name="textHint" format="string"></attr>
        <attr name="textDefaultValue" format="string"></attr>
        <attr name="android:inputType"/>
    </declare-styleable>

    <declare-styleable name="ProgressBarView">
        <attr name="textBigTitle" format="string"/>
        <attr name="textValue" format="string"/>
        <attr name="textLimit" format="string"/>
    </declare-styleable>
</resources>